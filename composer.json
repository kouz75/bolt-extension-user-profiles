{
    "name": "ohlandt/user-profiles",
    "description": "Extend the default Bolt users based on your needs and create public profile pages",
    "type": "bolt-extension",
    "keywords": [
        "user",
        "profiles",
        "social"
    ],
    "require": {
        "bolt/bolt": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Phillipp Ohlandt",
            "email": "phillipp.ohlandt@googlemail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Ohlandt\\UserProfiles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Ohlandt\\UserProfiles\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Ohlandt\\UserProfiles\\UserProfilesExtension"
    }
}
